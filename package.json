{
  "name": "lll-experience",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "lint": "tsc --noEmit",
    "db:generate": "drizzle-kit generate --schema=./src/db/schema.ts --dialect=postgresql",
    "db:studio": "drizzle-kit studio --dialect=postgresql",
    "db:migrate": "bun run src/db/migrate.ts"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.1.10",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/swagger": "^1.3.1",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.4.25",
    "elysia-clerk": "^0.13.2",
    "pino-pretty": "^13.1.3",
    "postgres": "^3.4.8"
  },
  "devDependencies": {
    "bun-types": "latest",
    "drizzle-kit": "^0.31.9"
  }
}
